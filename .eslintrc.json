{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "id-length": 0,
    "no-unreachable": 0,
    "no-ternary": 0,
    "no-process-env": 0,
    "no-magic-numbers": 0,
    "no-unused-vars": 1,
    "no-console": 1,
    "sort-vars": 0,
    "sort-keys": 0,
    "sort-imports": 0,
    "max-len": 0,
    "one-var": 0,
    "linebreak-style": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "react/jsx-sort-props": 0,
    "react/sort-prop-types": 0,
    "react/require-optimization": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0
  }
}
